<h1>All Teachers for <%= @category %></h1>

<% @hobbies.each do |hobby| %>
<div class="card">
<ul>
      <a href="<%=hobby_path(hobby)%>"><img src="<%=hobby.image_url%>" alt="Hobby Image" style="width:90%"><br></a>
      <div class ="container">
      <h4><%= hobby.name %></h4>
      <p><%= hobby.subclass %></p>
      </div>
</ul>
</div>
<% end %>
