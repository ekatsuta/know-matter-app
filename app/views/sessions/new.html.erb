<% if flash[:message] %>
<%= flash[:message] %>
<% end %>

<body id="login_page">


    <div class="login_container">

      <h1>Login</h1>

    <%= form_tag '/' do %>
    <%= label_tag "Username:" %>
    <%= text_field_tag :username %>
    <%= submit_tag "Login" %>
    <% end %>

    </div>


</body>
